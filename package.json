{
	"name": "@weekmeter/model",
	"version": "0.0.17",
	"description": "Model for the Weekmeter app",
	"author": "Weekmeter Contributors",
	"license": "MIT",
	"repository": "https://github.com/utily/library-template",
	"bugs": {
		"url": "https://github.com/utily/library-template/issues"
	},
	"homepage": "https://github.com/utily",
	"private": false,
	"main": "dist/index.js",
	"typings": "dist/index.d.ts",
	"type": "module",
	"git": {
		"tagName": "v${version}"
	},
	"github": {
		"release": true
	},
	"jest": {
		"transform": {
			"^.+\\.(j|t)sx?$": [
				"ts-jest",
				{
					"tsconfig": "./tsconfig.test.json"
				}
			],
			"^.+\\.(js)$": "babel-jest"
		},
		"transformIgnorePatterns": [
			"<rootDir>/node_modules/(?!(@userwidgets/model|flagly|userwidgets|cryptly|authly|isoly|gracely|cloudly-rest|cloudly-http|cloudly-router|cloudly-formdata|fetch-blob|formdata-polyfill|isly)/.*)"
		],
		"testEnvironment": "node",
		"testRegex": "((\\.|/)(test|spec))(\\.|\\/.+)(jsx?|tsx?)$",
		"testPathIgnorePatterns": [
			"node_modules/",
			"dist/"
		],
		"collectCoverageFrom": [
			"**/*.{ts,tsx,js,jsx}",
			"!**/node_modules/**",
			"!**/dist/**"
		]
	},
	"scripts": {
		"lint": "eslint --max-warnings=0 '**/*.{ts,tsx}'",
		"fix": "eslint '**/*.{ts,tsx}' --fix",
		"build": "tsc -p .",
		"test": "jest",
		"prepare": "npm run build",
		"clean": "rimraf dist node_modules coverage"
	},
	"devDependencies": {
		"@types/jest": "^29.5.5",
		"@typescript-eslint/eslint-plugin": "6.7.5",
		"@typescript-eslint/parser": "6.7.5",
		"eslint": "^8.51.0",
		"eslint-plugin-prettierx": "github:utily/eslint-plugin-prettierx#utily-20231004",
		"eslint-plugin-simple-import-sort": "^10.0.0",
		"jest": "^29.7.0",
		"prettierx": "github:utily/prettierx#utily-20231004",
		"rimraf": "^5.0.5",
		"ts-jest": "^29.1.1",
		"typescript": "^5.2.2"
	},
	"dependencies": {
		"@userwidgets/model": "^0.8.11",
		"isly": "^0.1.10",
		"isoly": "^2.0.25"
	},
	"overrides": {
		"semver": "7.5.3",
		"optionator": "0.9.3"
	}
}
